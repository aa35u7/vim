alias gc='git checkout'
alias gs='git status'
alias gd='git diff'
alias ga='git add'
alias gm='git commit'
alias gl='git log'
alias gw='git show'
alias gdt='git difftool -y -t meld'
alias gdth='git difftool -y -t meld HEAD^'
alias arb='adb reboot bootloader'
alias R='ctags -R'
alias lh='ll -h'
alias lsh='ls -Slh'
