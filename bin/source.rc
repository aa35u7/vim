if [ `uname -s` == "Linux" ]; then
	alias grep='$HOME/.vim/bin/tools/grep_linux --color=auto'
	alias astyle='$HOME/.vim/bin/tools/astyle_linux'
elif [ `uname -s` == "Darwin" ]; then
	alias grep='$HOME/.vim/bin/tools/grep_mac --color=auto'
	alias astyle='$HOME/.vim/bin/tools/astyle_mac'
fi


HISTTIMEFORMAT='%F %T  '

export USE_CCACHE=1
export CCACHE_DIR=$HOME/.ccache

