alias lindent='$HOME/.vim/bin/tools/lindent'
alias ..='cd ..'
alias c2='cd ../../'
alias c3='cd ../../..'
alias c4='cd ../../../..'
alias n='nautilus .'
alias cr='croot'
alias f='find ./ -iname'
alias repoall='repo forall -c '
alias log='cd $HOME/log/minicom_log/`LCALL=C date +%F`'
